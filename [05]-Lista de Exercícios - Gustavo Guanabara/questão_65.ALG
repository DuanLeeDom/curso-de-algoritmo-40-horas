algoritmo "questão_65"
var
   somatorio, C: Inteiro
inicio
      //Colocando variável
      C <- 0
      somatorio <- 100

      //contagem com loop para
      para C <- 0 ate 10 passo 1 faca
           Escreva(somatorio)
           somatorio <- somatorio - 10
      fimpara
      
      //Exibir resposta na tela
      Escreva(" Acabou!")
fimalgoritmo