algoritmo "questão_07"
var
   n1, dobro, tercaparte: real

inicio
      Escreva("Digite um número: ")
      Leia(n1)
      
      dobro <- n1*2
      
      tercaparte <- n1/3
      
      Escreval("O Dobro de ", n1, " é", dobro)
      Escreval("A terça parte de ", n1, " é", tercaparte)
fimalgoritmo