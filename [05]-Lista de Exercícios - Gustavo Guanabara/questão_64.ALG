algoritmo "questão_64"
var
   somatorio, C: Inteiro
inicio
      
      //loop de repetição usando para 5 em 5
      Para C <- 0 ate 8 passo 1 faca
           escreva(somatorio)
           somatorio <- somatorio + 5
      fimpara
      
      //exibir resposta na tela final
      escreva(" Acabou!")
fimalgoritmo