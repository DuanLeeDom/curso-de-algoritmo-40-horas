algoritmo "questão_38"
var
   N1, C: Inteiro
inicio
      //predefinir os indentificadores
      C <- 0
      N1 <- 6

      //Exibir os números de 6 até 11
      repita
            Escreva(N1)
            N1 <- N1 + 1
            C <- C + 1
      ate (C > 5)
      
      //Exibir na tela final
      Escreva(" Acabou!")
fimalgoritmo