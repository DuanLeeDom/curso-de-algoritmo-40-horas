algoritmo "questão_61"
var
   numero: Inteiro

inicio
   numero <- 0

   // Faça enquanto o número for menor ou igual a 30
   enquanto (numero <= 30) faca
      Escreva(numero)
      numero <- numero + 3
   fimenquanto

   // Exibir no final o resultado
   EscrevaL(" Acabou!")
fimalgoritmo
