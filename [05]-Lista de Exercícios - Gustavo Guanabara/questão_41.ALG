algoritmo "questão_41"
var
   C, N1: Inteiro
inicio
      //declaração das variaveis
      C <- 0
      N1 <- 100

      //repita até 20 até 100 - 5 chegar a 0
      Enquanto (C <= 20) faca
               Escreva(N1)
               C <- C + 1
               N1 <- N1 - 5
      Fimenquanto
      
      //Exibir na tela final
      Escreva(" Acabou!")
fimalgoritmo