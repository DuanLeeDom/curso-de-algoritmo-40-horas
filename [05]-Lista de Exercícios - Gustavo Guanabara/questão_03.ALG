algoritmo "questão_03"
var
   funcionario: caractere
   salario: real
inicio
      Escreva("Qual é seu nome?")
      Leia(Funcionario)
      
      Escreva("Qual é seu salário?")
      Leia(salario)
      
      Escreva("O funcionario ", funcionario, " tem um salário de R$", salario, " em junho")
fimalgoritmo