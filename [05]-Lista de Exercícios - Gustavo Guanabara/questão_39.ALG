algoritmo "questão_39"
var
   C, N1: Inteiro
inicio
      //predefinindo os indentificadores
      C <- 0
      N1 <- 10
      
      //fazer a repetição de 10 até 3
      repita
            Escreva(N1)
            N1 <- N1 - 1
            C <- C + 1
      ate (C > 7)
      
      //Exibir tela final
      Escreva(" Acabou!")
fimalgoritmo