algoritmo "questão_46"
var
   N1: Inteiro
inicio

      N1 <- 6
      
      //Comando para ir a soma de 1 até 100
      Enquanto (N1 <= 100) faca
              Escreva(N1)
              N1 <- N1 + 2
              se (N1 > 100) entao
                 Escreva(".")
              senao
                 Escreva(" +")
              fimse
      fimenquanto
fimalgoritmo