algoritmo "questão_67"
var
   resp, C: Inteiro
inicio
      //Comando de entrada
      Escreva("Digite um valor: ")
      Leia(resp)
      
      //Loop de contagem de acordo com o comando de entrada
      Escreva("Contagem: ")
      para C <- 0 ate resp passo 1 faca
      escreva(C, ",")
      fimpara
      
      //Exibir tela final
      Escreva(" FIM!")

fimalgoritmo